#!/bin/bash

set -e -u

cd `dirname $0`

output="../../GENERATED_LUA/GENERATED_LUA"

mkdir -p `dirname $output`
echo "/* the file generated by script, do not modify manually. */" > $output

for it in `find . -name "*.lua"`; do
    echo "\"`basename $it`\","       >> $output
    echo "R\"===(`cat $it`\n)===\"," >> $output
done
